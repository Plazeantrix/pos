Public Class M_LoanComakerAccount
    Private _FKAccountNumberComaker As String = ""
    Private _FKCoMakerID As String = ""
    Private _Flag As Boolean = False

    Public Sub Clear()
        _FKAccountNumberComaker = ""
        _FKCoMakerID = ""
        _Flag = False
    End Sub

    Public Property FKAccountNumberComaker() As String
        Get
            Return _FKAccountNumberComaker
        End Get
        Set(ByVal value As String)
            _FKAccountNumberComaker = value
        End Set
    End Property

    Public Property FKCoMakerID() As String
        Get
            Return _FKCoMakerID
        End Get
        Set(ByVal value As String)
            _FKCoMakerID = value
        End Set
    End Property

    Public Property Flag() As Boolean
        Get
            Return _Flag
        End Get
        Set(ByVal value As Boolean)
            _Flag = value
        End Set
    End Property


End Class
